{
  "name": "pkg-ok",
  "version": "3.0.0",
  "description": "Checks package.json main and bin paths",
  "exports": "./dist/index.js",
  "bin": "./dist/bin.js",
  "type": "module",
  "scripts": {
    "test": "npm run build && npm run jest && node ./dist/bin",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --write .",
    "build": "rm -rf dist && tsc",
    "lint": "prettier --check . && eslint . --ext .ts --ext .cjs --ext .mjs",
    "prepublishOnly": "npm test && node ./dist/bin",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/pkg-ok.git"
  },
  "keywords": [
    "package",
    "main",
    "bin",
    "field",
    "file",
    "path",
    "exist",
    "test",
    "crlf",
    "lf",
    "line ending"
  ],
  "author": "typicode <typicode@gmail.com>",
  "contributors": [
    "Abraham Williams <hi@abrah.am>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abraham/pkg-ok/issues"
  },
  "homepage": "https://github.com/abraham/pkg-ok#readme",
  "devDependencies": {
    "@types/mock-fs": "^4.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.1.0",
    "eslint-plugin-n": "^16.1.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "mock-fs": "^5.0.0",
    "prettier": "3.0.3",
    "type-fest": "^4.3.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "meow": "^11.0.0",
    "normalize-newline": "^4.0.0"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
